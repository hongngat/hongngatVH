var ASEProt=(document.location.protocol=='https:')?'https://':'http://';var ASEDel=ASEProt+'tag.gammaplatform.com/adx/';var ASEMedia=(document.location.protocol=='https:')?'https://gamma.cachefly.net/':'http://static.gammaplatform.com/';var gax_wrapper;var gaxLoadCode=0;function gaxInjectElementId(){gax_content_id=gax_content_id.replace("#","");var a=document.getElementById(gax_content_id).getElementsByTagName("p");gax_div_tag='p';if(a.length==0){a=document.getElementById(gax_content_id).getElementsByTagName("div");gax_div_tag='div'}for(i=0;i<a.length;i++){if(i==gax_position){var b=document.createElement('div');b.id="GaxVideoInreadPlayerWrapper";document.getElementById(gax_content_id).getElementsByTagName(gax_div_tag)[gax_position].appendChild(b);break}}};function gaxInjectElementClass(){gax_content_id=gax_content_id.replace(".","");var a=document.getElementsByClassName(gax_content_id)[0].getElementsByTagName("p");gax_div_tag='p';if(a.length==0){a=document.getElementsByClassName(gax_content_id)[0].getElementsByTagName("div");gax_div_tag='div'}for(i=0;i<a.length;i++){if(i==gax_position){var b=document.createElement('div');b.id="GaxVideoInreadPlayerWrapper";document.getElementsByClassName(gax_content_id)[0].getElementsByTagName(gax_div_tag)[gax_position].appendChild(b);break}}};function posY(a){var b=a,top=0;while(!!b&&b.tagName.toLowerCase()!=="body"){top+=b.offsetTop;b=b.offsetParent}return top};function scrollY(){if(window.pageYOffset){return window.pageYOffset}return Math.max(document.documentElement.scrollTop,document.body.scrollTop)};function viewPortHeight(){var a=document.documentElement;if(!!window.innerWidth){return window.innerHeight}else if(a&&!isNaN(a.clientHeight)){return a.clientHeight}return 0};function gaxCheckVisible(a){var b=viewPortHeight(),st=scrollY(),y=posY(a);h=a.clientHeight;ybottom=y+h;h_vib=h;val_if=document.getElementById("ifvideoinread").contentWindow;if((y>st-h_vib)&&(y<(st+b))&&(ybottom>st)&&(ybottom<(st+b)+h_vib)){console.log("visible");val_if.postMessage({func_type:'gaxPlay'},"*")}else{console.log("invisible");val_if.postMessage({func_type:'gaxPause'},"*")}};function loadVideoInreadPlayer(){var a=window.location.href;var b=a;var c=Math.floor((Math.random()*100000)+1);var d=ASEDel+'request/?wid='+gax_wid+'&zid='+gax_zid+'&player_width='+gax_w+'&player_height='+gax_h+'&cb='+c+'&content_page_url='+b;src=ASEMedia+"js/pc/js/outstream/jwplayer-8.7.1/out-stream-player.html?skipAd="+gax_skip+"&flash=false&tag="+encodeURIComponent(d)+"&media_path="+encodeURIComponent(ASEMedia)+"&w="+gax_w+"&h="+gax_h;document.getElementById('GaxVideoInreadPlayerWrapper').innerHTML="<iframe id='ifvideoinread' src='"+src+"' style='border:0;width:"+gax_w+"px;height:"+gax_h+"px' frameborder='0' scrolling='no'></iframe>"}function loadVideoInreadInit(){if(document.getElementById('GaxVideoInreadPlayerWrapper')==null){setTimeout("loadVideoInreadInit();",500)}else{gax_wrapper=document.getElementById('GaxVideoInreadPlayerWrapper');loadVideoInreadPlayer();gax_wrapper.style.height="0px";gax_wrapper.style.overflow="hidden";window.onscroll=function(e){gaxCheckVisible(gax_wrapper)}}};window.addEventListener('load',function(){console.log("inject");if(gax_position>0){id=gax_content_id.substr(0,1);if(id=="#"){gaxInjectElementId()}else{gaxInjectElementClass()}}loadVideoInreadInit()},true);if(gax_position==0){document.write('<div id="GaxVideoInreadPlayerWrapper"></div>')}window.addEventListener('message',function(a){data=a.data;if(data.func_type=='gaxShowOutstreamInreadPlayer'){gax_wrapper.style.width=data.param1;gax_wrapper.style.height=data.param2};if(data.func_type=='gaxCloseOutstreamInreadPlayer'){gax_wrapper.style.display='none'}},false);