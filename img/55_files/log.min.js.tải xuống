function checkvisit(e){try{var t="ePi.log.zonevisit",o=JSON.parse(getlogcookie(t)),n=Math.round((new Date).getTime()/1e3/60),r=!1,i=-1;if(null!=o&&o.length>0)for(var a=0;a<o.length;){if(o[a].z==e){n-o[a].d>=30&&(r=!0),i=a;break}a++}else o=new Array,r=!0;-1==i?(o.push({z:e,d:n}),r=!0):o[i].d=n}catch(c){o=new Array,o.push({z:e,d:n}),r=!0}return savelogcookie(t,JSON.stringify(o)),r}function checkview(e,t){if(0==e)return!1;try{var o="ePi.log.contentview",n=JSON.parse(getlogcookie(o)),r=Math.round((new Date).getTime()/1e3/60);if(null!=n&&n.length>0){for(var i=0;i<n.length&&n[i].d<r-5;)n.shift();n[n.length-1].d!=r&&n.push({d:r,v:","})}else n=new Array,n.push({d:r,v:","})}catch(a){n=new Array,n.push({d:r,v:","})}var c=!1;return JSON.stringify(n).toString().indexOf(","+e+"_"+t+",")>0?c=!1:(n[n.length-1].v+=e+"_"+t+",",c=!0),savelogcookie(o,JSON.stringify(n)),c}function checksitevisit(){var e=!1,t=0,o=Math.round((new Date).getTime()/1e3/60),n="ePi.log.sitevisit";try{t=parseInt(getlogcookie(n)),(isNaN(t)||o-t>=30)&&(e=!0)}catch(r){e=!0}return t=o,savelogcookie(n,o),e}function checkads(e,t){try{var o="ePi.log.ads2",n=JSON.parse(getlogcookie(o)),r=Math.round((new Date).getTime()/1e3/60),i=!1,a=-1;if(null!=n&&n.length>0)for(var c=0;c<n.length;){if(n[c].a==e+"_"+t){r-n[c].d>=5&&(i=!0),a=c;break}c++}else n=new Array,i=!0;-1==a?(n.push({a:e+"_"+t,d:r}),i=!0):n[a].d=r}catch(s){n=new Array,n.push({a:e+"_"+t,d:r}),i=!0}return savelogcookie(o,JSON.stringify(n)),i}function checkvote(e){try{var t="ePi.log.vote",o=JSON.parse(getlogcookie(t)),n=Math.round((new Date).getTime()/1e3/60),r=!1,i=-1;if(null!=o&&o.length>0)for(var a=0;a<o.length;){if(o[a].a==e){n-o[a].d>=60&&(r=!0),i=a;break}a++}else o=new Array,r=!0;-1==i?(o.push({a:e,d:n}),r=!0):o[i].d=n}catch(c){o=new Array,o.push({a:e,d:n}),r=!0}return savelogcookie(t,JSON.stringify(o)),r}function logging(e,t,o){var n=checkvisit(t),r=checksitevisit(),i=checkview(e,o),a=loghost+"/utilities/logcounter.ashx?type=view&zoneId="+t+"&contentId="+e;r&&(a+="&site=1"),n&&(a+="&zone=1"),i&&(a+="&content=1&img="+o),a+="&url="+window.encodeURIComponent(window.location.pathname),a+="&rd="+Math.random(1e6);var c=new Image;c.src=a}function logads(e,t){if(void 0==e&&(e=""),!checkads(e,t))return!1;if(void 0!=e&&""!=e&&NaN!=parseInt(e)){var o=loghost+"/utilities/logcounter.ashx?type=ads&ads="+e;o+="&rd="+Math.random(1e6);var n=new Image;n.src=o}return!0}function logvote(e,t){if(checkvote(e)){var o=loghost+"/utilities/logcounter.ashx?type=vote&id="+e+"&order="+t;o+="&rd="+Math.random(1e6);var n=new Image;n.src=o;var r="/ajax/tool.ashx?type=vote&id="+e+"&order="+t;r+="&rd="+Math.random(1e6);var i=new Image;i.src=r}}function getlogcookie(e){var t;return(t=new RegExp("(?:^|; )"+encodeURIComponent(e)+"=([^;]*)").exec(document.cookie))?t[1]:null}function savelogcookie(e,t){var o=new Date;o.setTime(o.getTime()+31536e6);var n="; expires="+o.toGMTString();document.cookie=e+"="+t+n+"; path=/;"}function logvotes(_voteIds, _orders) {if (!checkvote(_voteIds)) {return;}var url = loghost + '/utilities/logcounter.ashx?type=votes&ids=' + _voteIds + '&orders=' + _orders;url += "&rd=" + Math.random(1000000);var img = new Image(); img.src = url; var url2 = mainhost + '/ajax/tool.ashx?type=votes&ids=' + _voteIds + '&orders=' + _orders;url2 += "&rd=" + Math.random(1000000);var img2 = new Image(); img2.src = url2;}